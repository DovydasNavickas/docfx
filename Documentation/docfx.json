{
  "metadata": [
    {
      "src": [
        {
          "files": [ "**/*.csproj" ],
          "exclude": [ "**/bin/**", "**/obj/**" ],
          "cwd": "../src"
        }
      ],
      "dest": "obj/docfx/api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [ "**/*.yml" ],
        "cwd": "obj/docfx"
      },
      {
        "files": [ "tutorial/**/*.md", "spec/**/*.md", "spec/**/toc.yml", "index.md", "guideline/**/*.md" ]
      },
      {
        "files": [ "toc.yml", "tutorial/toc.yml" ]
      },
      {
        "files": [ "ReleaseNote.md" ],
        "cwd": ".."
      }
    ],
    "resource": [
      {
        "files": [ "spec/images/**", "tutorial/artifacts/**", "tutorial/images/**" ]
      }
    ],
    "overwrite": "apispec/*.md",
    "externalReference": [
    ],
    "globalMetadata": {
      "_appTitle": "docfx website"
    },
    "fileMetadata": {
      "priority": {
        "obj/docfx/**": 1.0,
        "**.md": 2.5,
        "spec/**.md": 3,
        "tutorial/**.md": 1.2
      },
      "keywords": {
        "obj/docfx/**": ["API", "Reference"],
        "spec/**.md": ["Spec", "Conceptual"],
        "**/*markdown.md": ["DFM", "Spec"]
      }
    },
    "dest": "_site",
    "template": [
      "default",
      "template"
    ]
  }
}